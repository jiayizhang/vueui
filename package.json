{
  "name": "@oovui/vueui",
  "version": "0.0.1",
  "description": "a UI components library based on vue",
  "author": "itshizhan@163.com",
  "keywords": [
    "vueui",
    "vuejs"
  ],
  "license": "MIT",
  "main": "dist/oovui.umd.js",
  "module": "dist/oovui.esm.js",
  "scripts": {
    "dev": "parcel ./demos/index.html",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "build:esm": "rollup --config build/rollup.config.esm.js",
    "build:lib": "npm run build:umd && npm run build:esm"
  },
  "repository": "https://github.com/oovui/vueui",
  "bugs": {
    "url": "https://github.com/oovui/vueui/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "vue": "^2.5.16",
    "vue-hot-reload-api": "^2.3.0"
  },
  "devDependencies": {
    "@oovui/vueui": "^0.0.1",
    "autoprefixer": "^8.6.5",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-vue": "^2.0.2",
    "clean-css": "^4.1.11",
    "minipass": "^2.3.3",
    "node-sass": "^4.9.2",
    "parcel-bundler": "^1.9.4",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-require-context": "^0.0.2",
    "rollup-plugin-vue": "^4.3.0",
    "sass-loader": "^7.0.3",
    "vue-template-compiler": "^2.5.16"
  }
}
